{
  "name": "open-draw",
  "version": "1.0.0",
  "packageManager": "pnpm@10.6.5",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "rm -rf ./dist && tsc -b && vite build",
    "preview": "vite preview --host",
    "import:fonts": "rm -rf ./public/fonts && cp -r ./node_modules/@excalidraw/excalidraw/dist/prod/fonts/ ./public",
    "integrate": "pnpm run upgrade:libs && pnpm run build",
    "upgrade:libs": "pnpm up --latest && pnpm install @excalidraw/excalidraw@next",
    "lint": "eslint ."
  },
  "dependencies": {
    "@excalidraw/excalidraw": "0.18.0-817d8c5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "eslint": "^9.23.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^6.2.2"
  }
}